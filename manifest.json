{
    "manifest_version": 2,
    "name": "Docsearch",
    "version": "1.1",
    "permissions": [
        "https://github.com/*",
        "https://gist.github.com/*"
    ],

    "description": "Show Function documentation (Python).",

    "content_scripts": [
        {
            "matches": [
                "https://github.com/*",
                "https://gist.github.com/*"
            ],
            "js": ["./source/main.js"]
        }
    ]
}